                       <div class="one-forth d-flex align-items-center ftco-animate" data-scrollax="properties: {translatev:'70%'}">
                        <div class="text">
                            <span class="subheading">Hello</span>
                            <h1 class="mb-4 mt-3">I'm <span>vikash kumar jha</span></h1>

                            <!--Element to contain animated typing-->
                            <span id="typing-animation"></span>

                            <script>

                                //Initialize the typing animation
                                constbtypingAnimationElement=document.getElementById('typing-animation');

                                //create an array of typing animation
                                const typingTexts=[
                                    'student ',
                                    'engeener',
                                ];

                                //create a function to display the typing animation for a given text
                                function PlayTypingAnimation(text) {
                                    //loop through each character and add it to the element
                                for(let i=0;i<text.length;i++) {
                                    setTimeout(()  =>  {
                                        typingAnimationElement.textcontent +=text[i];
                                    },  i=200); //increase the deley to slow down the typing animation
                                }


                                //once the animation is complete, reset the text and start over
                                setTimeout(() => {
                                    typingAnimationElement.textContent = "";
                                    PlayTypingAnimation(typingTexts[(typingTexts.indexof(text) +1) %typingTexts.length]);
                                }, text.length = 200);
                            }

                            //start the typing animation loop
                            PlayTypingAnimation(typingTexts[0]);

                            </script>

                            <br>
                            <br>
                            <h2>A B.Tech Student </h2>
                            <!--<h2 class="d-flex" style="margin-button: O">Hith Over 1 years of experience</h2> -->
                            <!--<br>-->
                            <p><a href="https://github.com/Vikash-Jha-Kumar" class="btn btn-primary py-3 px-4">My Works</a></p>
                        </div>
                       </div>
                </div>
